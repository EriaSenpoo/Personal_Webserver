@page
@model Personal_Website.Pages.Kontakt.IndexModel
@{
    ViewData["Title"] = "Index";
}

<div class="sub_buttons_specs">
    <a type="button" class="btn btn-outline-success sub_buttons_ind" style="color: #7d7d7d; border-color: #7d7d7d;" asp-area="" asp-page="/Om_mig/Index">Return</a>
</div>

<div class="data_position_title">
    <h1>Kontakt</h1>
</div>

<div class="data_position">
    @foreach (Personal_Website.Models.Data_text_object data in Model.list)
    {
        <div>
            <div>@data.subject</div>
            <div>@data.text</div>
        </div>
    }
</div>