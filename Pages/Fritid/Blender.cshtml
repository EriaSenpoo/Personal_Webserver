@page
@model Personal_Website.Pages.Fritid.BlenderModel
@{
    ViewData["Title"] = "Blender";
}

<img class="banner_position" src='@ViewData["Banner"]' alt="ViewData[Banner]" />

<div class="sub_buttons_specs">
    <a type="button" class="btn btn-outline-success sub_buttons_ind" style="color: #7d7d7d; border-color: #7d7d7d;" asp-area="" asp-page="/Fritid/Index">Return</a>
</div>

<div class="data_position_title">
    <h1>Blender</h1>
</div>

<div class="data_position">
    @foreach (Personal_Website.Models.Data_text_object data in Model.list)
    {
        <div>
            <div>@data.subject</div>
            <div>@data.text</div>
        </div>
    }
</div>